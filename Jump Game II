class Solution:
    def jump(self, nums):
        n = len(nums)
        jumps = 0
        cur_end = 0
        cur_farthest = 0

        for i in range(n - 1):
            # Update the farthest we can reach from current range
            cur_farthest = max(cur_farthest, i + nums[i])

            # If we reached the end of current jump, increase jump
            if i == cur_end:
                jumps += 1
                cur_end = cur_farthest

        return jumps
